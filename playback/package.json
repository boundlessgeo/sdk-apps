{
  "name": "sdk-app-playback",
  "version": "1.0.0",
  "description": "Playback example",
  "scripts": {
    "createdir": "node scripts/createbuild.js",
    "start:debug": "node debug-server.js",
    "start": "node_modules/.bin/npm-run-all --parallel createdir start:debug",
    "build:js": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js] app.jsx -o build/app.js",
    "build": "node_modules/.bin/npm-run-all --parallel createdir build:js"
  },
  "author": "Boundless Spatial, Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "boundless-sdk": "git+https://github.com/boundlessgeo/sdk.git",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "openlayers": "3.11.2",
    "react-intl": "2.0.0-beta-1"
  },
  "devDependencies": {
    "sdk-debug-server": "git+https://github.com/bartvde/sdk-debug-server.git",
    "babel-plugin-react-intl": "1.0.0-beta-5",
    "babelify": "^6.0.2",
    "browserify": "^11.2.0",
    "cssify": "^0.8.0",
    "envify": "^3.4.0",
    "fs-extra": "0.26.2",
    "minifyify": "^7.1.0",
    "npm-run-all": "1.3.1",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
}
