{
  "name": "sdk-app-basic",
  "version": "1.0.0",
  "description": "SDK application with a basic non-tabbed layout",
  "scripts": {
    "start": "node debug-server.js",
    "build": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js] app.jsx -o build/app.js"
  },
  "author": "Boundless Spatial, Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "boundless-sdk": "git+https://github.com/boundlessgeo/sdk.git",
    "pui-react-buttons": "3.0.0-alpha.1",
    "pui-react-iconography": "3.0.0-alpha.1",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "ol3-cesium": "3.9.0",
    "openlayers": "3.11.1",
    "react-intl": "2.0.0-beta-1"
  },
  "devDependencies": {
    "sdk-debug-server": "git+https://github.com/bartvde/sdk-debug-server.git",
    "babel-plugin-react-intl": "1.0.0-beta-5",
    "babelify": "^6.0.2",
    "browserify": "^11.2.0",
    "cssify": "^0.8.0",
    "envify": "^3.4.0",
    "minifyify": "^7.1.0",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
}
